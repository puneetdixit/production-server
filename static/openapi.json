            "in": "query",
            "name": "end_time",
            "schema": {
              "type": "string",
              "example": "2021-01-28T10:30:00Z"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "It will return a list of dict according to your filters",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "object",
                  "example": {"data": [], "status": "success"}
                }
              }
            }
          }
        }
      }
    },
